{
  "name": "fullstack",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./dist/bin/www",
    "dev": "DEBUG=fullstack NODE_ENV=development nodemon --exec babel-node ./server/bin/www",
    "devc": "eslint --fix ./client && NODE_ENV=development && webpack serve --config ./webpack.dev.config.js --mode development",
    "devb": "babel-watch ./server/bin/www",
    "cleans": "rm -rf dist",
    "cleanc": "rm -f ./public/bundle.js ./public/stylesheets/app.css",
    "b:s": "npm run cleans && babel ./server --out-dir dist --copy-files",
    "b:c": "npm run cleanc && webpack --config ./webpack.config.js --mode production",
    "itgam": "ls",
    "build": "npm run b:c && npm run b:s",
    "clear-logs": "rm -rf ./server/logs/*.log"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-handlebars": "^5.3.2",
    "google-icon": "^1.0.1",
    "hbs": "^4.1.1",
    "http-errors": "~1.6.3",
    "material-icons": "^1.12.1",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^6.0.5",
    "morgan": "~1.9.1",
    "rimraf": "^3.0.2",
    "webpack": "^5.35.0",
    "webpack-dev-middleware": "^4.2.0",
    "webpack-hot-middleware": "^2.25.0",
    "winston": "^3.3.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-minify": "^0.5.1",
    "babel-watch": "^7.4.1",
    "core-js": "^3.11.1",
    "css-loader": "^5.2.4",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-webpack-plugin": "^3.1.1",
    "mini-css-extract-plugin": "^1.6.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.4.1",
    "style-loader": "^3.3.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^4.11.0"
  }
}
